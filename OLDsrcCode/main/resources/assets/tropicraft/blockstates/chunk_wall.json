{
  "multipart": [
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "tropicraft:block/chunk_wall_post"
      }
    },
    {
      "when": {
        "north": "low"
      },
      "apply": {
        "model": "tropicraft:block/chunk_wall_side",
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "low"
      },
      "apply": {
        "model": "tropicraft:block/chunk_wall_side",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "low"
      },
      "apply": {
        "model": "tropicraft:block/chunk_wall_side",
        "y": 270,
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "low"
      },
      "apply": {
        "model": "tropicraft:block/chunk_wall_side",
        "y": 90,
        "uvlock": true
      }
    }
  ]
}